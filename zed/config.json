{
  "theme": "Catppuccin Mocha",
  "ui": {
    "font_size": 14,
    "font_family": "JetBrainsMono Nerd Font",
    "line_height": 1.5
  },
  "editor": {
    "soft_wrap": "editor_width",
    "tab_size": 2,
    "insert_spaces": true,
    "auto_indent": true,
    "rulers": [80],
    "cursor_blink": true
  },
  "terminal": {
    "font_size": 14,
    "font_family": "JetBrainsMono Nerd Font",
    "working_directory": "current"
  },
  "languages": {
    "javascript": {
      "formatter": "prettier",
      "prettier": {
        "singleQuote": true,
        "trailingComma": "es5"
      }
    },
    "typescript": {
      "formatter": "prettier",
      "prettier": {
        "singleQuote": true,
        "trailingComma": "es5"
      }
    },
    "json": {
      "formatter": "prettier"
    },
    "markdown": {
      "formatter": "prettier"
    }
  },
  "features": {
    "copilot": true,
    "lsp": true
  },
  "language_models": {
    "anthropic": {
      "api_url": "https://api.z.ai/api/anthropic",
      "available_models": [
        {
          "name": "glm-4.6",
          "display_name": "GLM-4.6",
          "max_output_tokens": 24480,
          "max_tokens": 200000
        },
        {
          "name": "glm-4.5-air",
          "display_name": "GLM 4.5 Air",
          "max_output_tokens": 96000,
          "max_tokens": 131100
        }
      ]
    }
  },
  "agent": {
    "default_profile": "ask",
    "default_model": {
      "provider": "anthropic",
      "model": "glm-4.6"
    },
    "model_parameters": []
  },
  "base_keymap": "JetBrains",
  "ui_font_size": 16,
  "buffer_font_size": 16,
  "theme": {
    "mode": "system",
    "light": "One Light",
    "dark": "Solarized Dark"
  }
}
